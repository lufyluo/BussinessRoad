.container .box .list{width:100%;border-bottom:1rpx #d9d9d9 solid;padding:20rpx;color:#111010;font-size:28rpx;line-height:40rpx;position:relative;}
.container .box .list .btn1{position:absolute;right:120rpx;top:20rpx;width:50rpx;height:50rpx;background:url(http://bjcreate.chinacloudapp.cn/xiaochengxu/email/phone1.png) no-repeat 0 0;background-size:100%;color:transparent;}
.container .box .list .btn2{position:absolute;right:30rpx;top:20rpx;width:50rpx;height:50rpx;background:url(http://bjcreate.chinacloudapp.cn/xiaochengxu/email/email1.png) no-repeat 0 0;background-size:100%;color:transparent;}